{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "Rms",
	"patterns": [
		{
			"include": "#functions"
		},
		{
			"include": "#keywords"
		},
		{
			"include": "#strings"
		},
		{
			"include": "#numbers"
		},
		{
			"include": "#comments"
		}
	],
	"repository": {
		"functions": {
			"patterns": [{
				"name": "entity.name.function.rms",
				"match": "\\b(ANY|any|TEXT|text|VARCHAR|varchar|repeat|REPEAT|GROUP|group)\\b"
			}]
		},
		"keywords": {
			"patterns": [{
				"name": "keyword.control.rms",
				"match": "\\b(INLINE|inline|OR|or|MORE|START|END)\\b"
			}]
		},
		"strings": {
			"name": "string.quoted.double.rms",
			"begin": "[@]{0,1}\"",
			"end": "\"",
			"patterns": [
				{
					"name": "constant.character.escape.rms",
					"match": "\\\\."
				}
			]
		},
		"numbers": {
			"patterns": [{
				"name": "constant.numeric.rms",
				"match": "[0-9]|[\\\\*]|[\\\\+]|[\\\\?]"
			}]
		},
		"comments": {
			"name": "comment.line.number-sign.rms",
			"begin": "(\\#)",
			"beginCaptures": {
				"1": {
					"name": "punctuation.definition.comment.rms"
				}
			},
			"end": "($)"
		}
	},
	"scopeName": "source.rms"
}